{
  "name": "plugin-arc",
  "version": "1.0.0",
  "description": "A plugin-based modular Node.js system with dynamic plugin loading and lifecycle management",
  "private": true,
  "main": "dist/core/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/core/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/core/server.js",
    "prisma:generate": "prisma generate --config prisma/prisma.config.ts",
    "prisma:migrate:dev": "prisma migrate dev --config prisma/prisma.config.ts",
    "prisma:migrate:deploy": "prisma migrate deploy --config prisma/prisma.config.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@prisma/client": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.4",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.1",
    "rate-limit-redis": "^2.1.0",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.2",
    "@types/rate-limit-redis": "^1.7.4",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^30.2.0",
    "prettier": "^3.2.0",
    "prisma": "^7.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "plugin-architecture",
    "modular",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
